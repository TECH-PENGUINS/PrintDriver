[CLEAN_LINKS.NT6]
"REMOVE_BY_EXECUTABLE" = "KmUninstall.exe","<StartMenu>\UTAX TA"
"REMOVE_BY_EXECUTABLE" = "KmUninstallVista.exe","<StartMenu>\UTAX TA"
"REMOVE_BY_EXECUTABLE" = "KmUninstall.exe","<StartMenu>\TA"
"REMOVE_BY_EXECUTABLE" = "KmUninstallVista.exe","<StartMenu>\TA"	

[DIRS]
<MainTarget>\Utility,REMOVE_ALL
<MainLink>

[TA_LINK_DIR_CLEAN]
<StartMenu>\TA

[TA_ARP_CLEAN]
"TA Product Library","TA Product Library",<MainTarget>\KmUninstall.exe -l <UiLanguage> -ck KYOCERA,KYOCERA Document Solutions Inc.,<MainTarget>,<MainTarget>\KmUninstall.exe


[DOC_CLEANUP]
<MainTarget>\Documents,REMOVE_ALL
<MainLink>\Documents,REMOVE_ALL

[AppInstall]
InstallCmd = "Setup/KmCustom32.exe RESTORE_LM -n ''"

[AppInstall.x64]
