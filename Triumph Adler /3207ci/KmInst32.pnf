DelDirs = TA_LINK_DIR_CLEAN
DelItemARP = TA_ARP_CLEAN
CopyFiles = SEC1,BMP
AddLinks = LINK1
AddItemARP = ARP
ExecFiles = AppInstall

[Remove]
