DelFiles = BMP,SEC1
DelLinks = LINK1
DelItemARP = ARP
DelDirs = DIRS
DelDirs = DOC_CLEANUP

[SEC1]
KmUninstall.exe,,KmUninstall.exe,COPY_NEW_FILE
KmInst32.exe,,KmInst32.exe,COPY_NEW_FILE
KmInst64.exe,,KmInst64.exe,COPY_NEW_FILE
KyoRm.ini,,KyoRm.ini,COPY_NEW_FILE
KmInstall.str
KmInst32.str
Lang.dat

[BMP]
KxLogo.png

[LINK1]
%PRODUCT_NAME% = "<MainTarget>\KmUninstall.exe [-l <UiLanguage>]","<MainLink>","<MainTarget>\KmUninstall.exe",%LINK_DESC%

[ARP]
%DISPLAY_NAME%,%DISPLAY_NAME%,<MainTarget>\KmUninstall.exe -l <UiLanguage> -ck KYOCERA,KYOCERA Document Solutions Inc.,<MainTarget>,<MainTarget>\KmUninstall.exe

[CLEAN_LINKS]
