[Version]
Provider = UTAX TA

Version  = 08/12/2022,6.0.4912

[Information]
PackageKey = KMINST32
PackageType = KMINST32
Category = UTILITY
Name = "Uninstaller"
Description = "KmUninstaller"
Location = 
MinOSVersion = 5.0
MaxOSVersion = 10.0
Connection = USB,NET
Attribute = HIDE,ALWAYSINSTALL
Required = 
Handler = DriverDll.dll
SourcePath = .
TargetPath = <MainTarget>
IncludeARMDisplay = 1

[Default]

[Private]


[SourcePath]
Default = .
BMP = Setup

[TargetPath]
